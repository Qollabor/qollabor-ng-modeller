<process id="id_WjAF" name="allocatefunds.process" description="allocatefunds" implementationType="http://www.omg.org/spec/CMMN/ProcessType/Unspecified">
    <input id="travelrequest" name="travelrequest"/>
    <input id="approval" name="approval"/>
    <output id="purchaseorder" name="purchaseorder"/>
    <extensionElements>
        <qollabor:implementation xmlns:qollabor="org.qollabor" class="org.qollabor.processtask.implementation.http.HTTPCallDefinition">
            <url>http://localhost:8380/allocatefunds</url>
            <method>POST</method>
            <post-content>{"travelrequest": "${travelrequest}", "approval": "${approval}"}</post-content>
            <http-headers>
                <http-header name="Content-Type">application/json</http-header>
            </http-headers>
            <parameterMapping sourceRef="responsePayload" targetRef="purchaseorder"/>
        </qollabor:implementation>
    </extensionElements>
</process>